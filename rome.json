{
	"$schema": "./node_modules/rome/configuration_schema.json",
	"javascript": {
		"formatter": {
			"quoteStyle": "single"
		}
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"lineWidth": 120
	},
	"linter": {
		"enabled": true,
		"rules": {
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	}
}
