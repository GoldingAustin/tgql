{
	"name": "@tgql/client",
	"description": "GraphQL Client for @tgql/server (without any file generation)",
	"version": "0.0.0-alpha.0",
	"author": {
		"name": "Austin Golding",
		"email": "austin.golding.dev@icloud.com"
	},
	"contributors": [
		{
			"name": "Devan Farrell"
		}
	],
	"bugs": {
		"url": "https://github.com/GoldingAustin/tgql/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GoldingAustin/tgql"
	},
	"license": "MIT",
	"main": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": {
				"default": "./dist/index.mjs",
				"types": "./src/index.ts"
			},
			"require": {
				"default": "./dist/index.js",
				"types": "./src/index.ts"
			},
			"types": "./src/index.ts"
		}
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE",
		"package.json"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsup-node ./src/index.ts --clean --minify --format esm,cjs",
		"test": "bun test",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"graphql": "^16"
	}
}
